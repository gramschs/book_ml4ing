y = np.linspace(ylim[0], ylim[1], 30)\n    Y, X = np.meshgrid(y, x)\n    xy = np.vstack([X.ravel(), Y.ravel()]).T\n\n    # Abstand zur Trennhyperebene berechnen mit eingebauter \n    # decision_function()\n    P = model.decision_function(xy).reshape(X.shape)\n\n    # Entscheidungsgrenzen und Margin darstellen\n    ax.contour(X, Y, P, colors='k', levels=[-1, 0, 1], alpha=0.5, linestyles=['--', '-', '--'])\n\n    # Stützvektoren darstellen\n    ax.scatter(model.support_vectors_[:, 0], model.support_vectors_[:, 1], s=300, linewidth=1, facecolors='none', edgecolors='orange');\n    ax.set_xlim(xlim)\n    ax.set_ylim(ylim)","key":"xU0JVx3GGX"},{"type":"outputs","id":"9i_jza0iTfXfa72ZI6Jpc","children":[],"key":"B5ih2kEw1G"}],"key":"LsDU514bgr"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"fig, ax = plt.subplots()\nax.scatter(X[:,0], X[:,1], c=y, cmap='coolwarm')\nax.set_xlabel('Feature 1')\nax.set_ylabel('Feature 2')\nax.set_title('SVM mit Soft Margin');\n\nplot_svc_grenze(svm_modell)","key":"UjctPFrsLe"},{"type":"outputs","id":"dCHMhfxWe4K53vPgnF7c3","children":[{"type":"output","children":[],"jupyter_data":{"output_type":"display_data","metadata":{},"data":{"text/plain":{"content":"<Figure size 640x480 with 1 Axes>","content_type":"text/plain"},"image/png":{"content_type":"image/png","hash":"5b4154fff41fcc2ccc7fbe05ffe36009","path":"/book_ml4ing/build/5b4154fff41fcc2ccc7fbe05ffe36009.png"}}},"key":"M4MEZJoz2r"}],"key":"p4GdMttV4W"}],"key":"yivC2vqjqo"},{"type":"block","children":[{"type":"heading","depth":2,"position":{"start":{"line":138,"column":1},"end":{"line":138,"column":1}},"children":[{"type":"text","value":"Der Parameter C","position":{"start":{"line":138,"column":1},"end":{"line":138,"column":1}},"key":"jvtkinYU4N"}],"identifier":"der-parameter-c","label":"Der Parameter C","html_id":"der-parameter-c","implicit":true,"key":"wuX7igOkqs"},{"type":"paragraph","position":{"start":{"line":140,"column":1}